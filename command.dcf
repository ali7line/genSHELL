*FILOS
 INITIA
*input
*NONLIN LABEL="Structural nonlinear"
  MODEL ASSEMB TOLERA 0.001
  BEGIN EXECUT
    BEGIN LOAD
      LOADNR 1
      STEPS EXPLIC
    END LOAD
    BEGIN ITERAT
      MAXITE 30
      METHOD NEWTON
      LINESE
      BEGIN CONVER
        FORCE TOLCON 1e-05
        DISPLA TOLCON 1e-05
      END CONVER
    END ITERAT
    SOLVE PARDIS MAXTHR 10
  END EXECUT
  SOLVE PARDIS
  BEGIN OUTPUT NDIANA
    TEXT "Output"
    BINARY
    FILE "aaa"
    SELECT STEPS ALL /
  END OUTPUT
*END
