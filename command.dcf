*FILOS
 INITIA
*input
*NONLIN LABEL="Structural nonlinear"
  MODEL ASSEMB TOLERA 0.001
  BEGIN EXECUT
    BEGIN LOAD
      LOADNR 1
      STEPS EXPLIC SIZES 1 (1)
    END LOAD
    BEGIN ITERAT
      MAXITE 30
      METHOD NEWTON
      LINESE
      BEGIN CONVER
        FORCE TOLCON 1e-05
        DISPLA TOLCON 1e-05
      END CONVER
    END ITERAT
    SOLVE PARDIS MAXTHR 10
  END EXECUT
  SOLVE PARDIS
  BEGIN OUTPUT NDIANA
    TEXT "Output"
    ASCII
    FILE "output.dat"
    BEGIN SELECT
      BEGIN STEPS ALL /
        MIN
        MAX
      END STEPS
    END SELECT
    DISPLA TOTAL TRANSL GLOBAL X Y Z
  END OUTPUT
*END
